    function commonMethodForAllOne(txtId, retriveValue) {
        //alert(txtId + " " + retriveValue);

        if (Helper.IsNumeric(retriveValue) == false) {
            $("#" + txtId + "").val('0');
            alert("টাকার পরিমান অংকে লিখুন ");
            return false;
        }
        else if (retriveValue < 0) {
            $("#" + txtId + "").val('0');
            alert("টাকার পরিমান মাইনাস হবে না ");
            return false;
        }
        var txtSolarMulProject = parseInt($('#txtSolarMulProject').val().trim(), 10);
        var txtSolarIdclProject = parseInt($('#txtSolarIdclProject').val().trim(), 10);
        var txtKhusraMlBikrySolar = parseInt($('#txtKhusraMlBikrySolar').val().trim(), 10);
        var txtBiogazProject = parseInt($('#txtBiogazProject').val().trim(), 10);
        var txtTechFeeBiogaz = parseInt($('#txtTechFeeBiogaz').val().trim(), 10);
        var txtKhusraMlBikryBiogaz = parseInt($('#txtKhusraMlBikryBiogaz').val().trim(), 10);
        var txtJnTechTkPrapti = parseInt($('#txtJnTechTkPrapti').val().trim(), 10);
        var txtUpVaraPrapti = parseInt($('#txtUpVaraPrapti').val().trim(), 10);
        var txtJTCSecurityJoma = parseInt($('#txtJTCSecurityJoma').val().trim(), 10);
        var txtPrakritikDurjogTohbilePrapti = parseInt($('#txtPrakritikDurjogTohbilePrapti').val().trim(), 10);
        var txtTatkhonikCkriObatiBbdPrapti = parseInt($('#txtTatkhonikCkriObatiBbdPrapti').val().trim(), 10);
        var txtBondhuBati = parseInt($('#txtBondhuBati').val().trim(), 10);
        var txtOuditApttiTkPraptiNam = parseInt($('#txtOuditApttiTkPraptiNam').val().trim(), 10);
        var txtOnnoUnitThekePraptiUniterNam = parseInt($('#txtOnnoUnitThekePraptiUniterNam').val().trim(), 10);
        var txt = parseInt($('#txt').val().trim(), 10);


        var sumOfAll = parseInt(txtSolarMulProject) + parseInt(txtSolarIdclProject) + parseInt(txtKhusraMlBikrySolar) + parseInt(txtBiogazProject)
                        + parseInt(txtTechFeeBiogaz) + parseInt(txtKhusraMlBikryBiogaz) + parseInt(txtJnTechTkPrapti) + parseInt(txtUpVaraPrapti)
                        + parseInt(txtJTCSecurityJoma) + parseInt(txtPrakritikDurjogTohbilePrapti) + parseInt(txtTatkhonikCkriObatiBbdPrapti)
                        + parseInt(txtBondhuBati) + parseInt(txtOuditApttiTkPraptiNam) + parseInt(txtOnnoUnitThekePraptiUniterNam) + parseInt(txt);

        // alert(sumOfAll);
        $('#txtBortomanMaseMotAday').val(sumOfAll);
        var txtBortomanMaseMotAday = $('#txtBortomanMaseMotAday').val(sumOfAll);
        var txtBigotomaserHateNogod = $('#txtBigotomaserHateNogod').val().trim();
        var MotTakarPoriman = parseInt(sumOfAll, 10) + parseInt(txtBigotomaserHateNogod, 10);
        $('#txtMotTakarPoriman').val(MotTakarPoriman);
        var txtMotTakarPorimanGet = $('#txtMotTakarPoriman').val();
        var txtMotBankeJomaKhoroserPorimanGet = $('#txtMotBankeJomaKhoroserPoriman').val();
        var masSeseHateNogod = parseInt(txtMotTakarPorimanGet, 10) - parseInt(txtMotBankeJomaKhoroserPorimanGet, 10);
        $('#txtMasSeseHateNogod').val(masSeseHateNogod);

    }

