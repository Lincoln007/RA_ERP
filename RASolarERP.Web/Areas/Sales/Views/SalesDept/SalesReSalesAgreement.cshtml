                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                cache: false,
                success: function (data) {

                    var modelCounter = 0;
                    var modelSelected = "";

                    for (var i = 0; i < rowsCount; i++) {

                        var tr = $("#gvPackage tbody tr:eq(" + i + ")");
                        var categoryId = $(tr).find('td:eq(0)').text();
                        var isSelectOrNot = $(tr).find('td:eq(3)').find('select');
